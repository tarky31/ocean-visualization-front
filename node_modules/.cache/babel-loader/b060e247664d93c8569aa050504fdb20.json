;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAiPA;AACAA,MADA,kBACA;AACA;AACAC,gBADA;AACA;AACAC,gBAFA;AAEA;AACAC,eAHA;AAGA;AACAC,kBAJA;AAKAC,kBALA;AAMAC,kBANA;AAOAC;AACAH,uBADA;AAEAI;AAFA,SAGA;AACAJ,uBADA;AAEAI;AAFA,OAHA,EAMA;AACAJ,uBADA;AAEAI;AAFA,OANA,EASA;AACAJ,uBADA;AAEAI;AAFA,OATA,EAYA;AACAJ,uBADA;AAEAI;AAFA,OAZA;AAPA;AAyBA,GA3BA;AA6BAC;AACAC,cADA,wBACA;AAAA;;AACA;AACAC,+BADA;AAEAC;AACA;AACAC,wBADA;AAEAC;AAFA;AAIA;AAPA;AASA;AAXA;AA7BA","names":["data","value1","value2","radio","wp_value","na_value","ep_value","options","label","methods","fileSubmit","confirmButtonText","callback","type","message"],"sourceRoot":"views","sources":["Typhoon.vue"],"sourcesContent":["<template>\r\n<div style=\"width:100%;height:100%;background-color: #F3F2F3;\">\r\n     <div class=\"box\">\r\n        <div id=\"dashboard\">\r\n            <div id=\"data_upload\" class=\"part\">\r\n                <div class=\"title\">\r\n                    <h4>\r\n                       台风强度智能预报结果可视化\r\n                    </h4>\r\n                </div>\r\n                <div class=\"center\">\r\n                    <input type=\"button\" value=\"导入台风数据\" id=\"data_btn\" @click=\"fileSubmit\">\r\n                </div>\r\n            </div>\r\n            <div class=\"part\">\r\n                <div class=\"title\" >\r\n                    <h4>\r\n                        选择日期\r\n                    </h4>   \r\n                    <div class=\"container\">\r\n                        <el-date-picker\r\n                            v-model=\"value1\"\r\n                            type=\"date\"\r\n                            placeholder=\"选择日期\"\r\n                            format=\"yyyy-MM-dd\"\r\n                            value-format=\"yyyy-MM-dd\">\r\n                        </el-date-picker>\r\n                        <el-time-select\r\n                            v-model=\"value2\"\r\n                            :picker-options=\"{\r\n                                start: '00:00',\r\n                                step: '01:00',\r\n                                end: '23:00'\r\n                            }\"\r\n                            placeholder=\"选择时间\">\r\n                        </el-time-select>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n            \r\n            <el-divider></el-divider>\r\n            <div style=\"margin-bottom: 5%;\" class=\"part\">  <!--台风-->\r\n                <div class=\"title\">\r\n                    <h4>\r\n                        请选择对应\r\n                    </h4>\r\n                </div>\r\n                <div style=\"margin-left:5%;\">\r\n                    <div class=\"type\">\r\n                        <el-radio v-model=\"radio\" label=\"wp\">WP</el-radio>\r\n                        <div class=\"select\">\r\n                            <el-select v-model=\"wp_value\" placeholder=\"请选择\">\r\n                                <el-option\r\n                                v-for=\"item in options\"\r\n                                :key=\"item.value\"\r\n                                :label=\"item.label\"\r\n                                :value=\"item.value\">\r\n                                </el-option>\r\n                            </el-select>\r\n                        </div>\r\n                       \r\n                    </div>\r\n                    <div class=\"type\">\r\n                        <el-radio v-model=\"radio\" label=\"na\">NA</el-radio>\r\n                        <div class=\"select\">\r\n                            <el-select v-model=\"na_value\" placeholder=\"请选择\">\r\n                                <el-option\r\n                                v-for=\"item in options\"\r\n                                :key=\"item.value\"\r\n                                :label=\"item.label\"\r\n                                :value=\"item.value\">\r\n                                </el-option>\r\n                            </el-select>\r\n                        </div>    \r\n                    </div>\r\n                    <div class=\"type\">\r\n                        <el-radio v-model=\"radio\" label=\"ep\">EP</el-radio>\r\n                        <div class=\"select\">\r\n                            <el-select v-model=\"ep_value\" placeholder=\"请选择\">\r\n                                <el-option\r\n                                v-for=\"item in options\"\r\n                                :key=\"item.value\"\r\n                                :label=\"item.label\"\r\n                                :value=\"item.value\">\r\n                                </el-option>\r\n                            </el-select>\r\n                        </div>    \r\n                    </div>\r\n                </div>\r\n            </div> \r\n                \r\n            <div style=\"margin-top: 8%;\">\r\n                <div class=\"center\">\r\n                    <input type=\"button\" value=\"渲染\" id=\"draw_btn\">\r\n                </div>\r\n            </div>\r\n           \r\n        </div>\r\n        <div class=\"vis-content\">   <!--可视化区域-->\r\n          \r\n        </div>\r\n    </div>\r\n</div>\r\n    \r\n</template>\r\n\r\n<style>\r\nbody,html{\r\n    height:100%;\r\n    background-color: #F3F2F3;\r\n    margin: 0;\r\n    /* text-align: center; */\r\n}\r\n\r\n.box{\r\n    /* display: inline-block; */\r\n    width: 80%;\r\n    height: 100%;\r\n    margin: 0 auto;\r\n}\r\n\r\n.box .title{\r\n    margin-left: 3%;\r\n}\r\n\r\n.box .container{\r\n    width: 100%;\r\n    text-align: center;\r\n    margin-left: -2%;\r\n\r\n}\r\n\r\n#dashboard{\r\n    float: left;\r\n    height: calc(100% - 10px);\r\n    width: 30%;\r\n    /* border: 1px solid #D2D2D2; */\r\n    margin-top: 10px;\r\n    background-color: white;\r\n}\r\n\r\n.box .vis-content{\r\n    float: left;\r\n    height: calc(100% - 10px);\r\n    width: 70%;\r\n    margin-top:10px;\r\n    background-color: white;\r\n    display: flex;\r\n    flex-wrap: wrap;\r\n}\r\n\r\n.box .vis-content .vis{\r\n    width: 33%;\r\n    height: 50%;\r\n}\r\n\r\n#dashboard #sub_btn{\r\n    width: 131px;\r\n    height: 33px;\r\n    color: white;\r\n    cursor: pointer;\r\n    background: #4472C4;\r\n    border: none;\r\n    outline: none;\r\n    font-size: 14px;\r\n    border-radius: 5px;\r\n}\r\n\r\n#dashboard .squ-checkbox{\r\n    margin-right:3%;\r\n}\r\n#dashboard .type{\r\n    display: block;\r\n    margin-bottom:10% ;\r\n}\r\n\r\n#dashboard .select{\r\n    padding-left: 12%;\r\n    /* margin-bottom: 5%; */\r\n    margin:4%;\r\n}\r\n\r\n\r\n#dashboard .center{\r\n    /* margin-left: -15%; */\r\n    margin-top: 3%;\r\n    text-align:center\r\n}\r\n\r\n#dashboard #draw_btn,#dashboard #data_btn{\r\n    width: 80%;\r\n    height: 33px;\r\n    color: white;\r\n    cursor: pointer;\r\n    background: #4472C4;\r\n    border: none;\r\n    outline: none;\r\n    font-size: 14px;\r\n    border-radius: 5px;\r\n}\r\n\r\n.container #date{\r\n    float: left;\r\n    margin-left: 2%;\r\n    height: 30px;\r\n    line-height: 30px;\r\n    margin-right: 1%;\r\n}\r\n\r\n.container .form-horizontal{\r\n    float: left;\r\n}\r\n\r\n.el-input__inner {\r\n    height: 33px;\r\n    line-height: 33px;  \r\n    border: #038FBE 1.5px solid;\r\n}\r\n.el-input__icon {\r\n    line-height: 33px;\r\n}\r\n.el-date-editor.el-input{\r\n    width: 150px;\r\n    margin: 0 3px;\r\n}\r\n\r\n.el-radio {\r\n    font-weight: 700;\r\n}\r\n\r\n.el-radio__label,.el-checkbox__label {\r\n    font-size: 15px;\r\n}\r\n\r\n.el-divider--horizontal{\r\n    width: 90%;\r\n    margin: 24px auto\r\n}\r\n</style>\r\n\r\n<script>\r\n  export default {\r\n    data() {\r\n      return {\r\n        value1:'',   //日期\r\n        value2:'',   //时刻\r\n        radio: '',  //选择的属性\r\n        wp_value: '',\r\n        na_value:'',\r\n        ep_value:'',\r\n        options: [{\r\n          wp_value: '选项1',\r\n          label: '黄金糕'\r\n        }, {\r\n          wp_value: '选项2',\r\n          label: '双皮奶'\r\n        }, {\r\n          wp_value: '选项3',\r\n          label: '蚵仔煎'\r\n        }, {\r\n          wp_value: '选项4',\r\n          label: '龙须面'\r\n        }, {\r\n          wp_value: '选项5',\r\n          label: '北京烤鸭'\r\n        }],\r\n        \r\n      };\r\n    },\r\n\r\n     methods: {\r\n      fileSubmit() {\r\n        this.$MessageBox.alert('请将nc文件放置到home/user_ocean_data,然后点击确定', '提示', {\r\n          confirmButtonText: '确定',\r\n          callback: action => {\r\n            this.$message({\r\n              type: 'info',\r\n              message: `action: ${ action }`\r\n            });\r\n          }\r\n        });\r\n      },\r\n     \r\n      \r\n     }\r\n  }\r\n</script>"]},"metadata":{},"sourceType":"module"}